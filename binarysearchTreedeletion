def deletion(self,data):
    if self.key==None:
        return "empty"
    if data>self.key:
        if self.rchild:
            self.rchild.deletion(data)
        else:
            return "empty"
    elif data<self.key:
        if self.lchild:
            self.lchild.deletion(data)
        else:
            return "not found"
    else:
        if self.lchild is None:
            temp=self.rchild
            self=None
            return temp
        if self.rchild is None:
            temp=self.lchild
            self=None
            return temp
        node=self.rchild
        while node.lchild:
            node=node.lchild
        self.key=node.key
        self.rchild=self.rchild.delete(node.key)
    
